void mostrar( Conjunto c)
{
	List<char> unaLista=c.dameListaElementos();

	for (int i=0;i<unaLista.size();i++)
	{
		char unaLetra=unaLista.at(i);
		cout << " ";
		cout << unaLetra;
	}
cout <<"\n";
}


